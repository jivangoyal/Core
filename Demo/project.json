{
  "name": "Demo",
  "version": "0.0.6",
  "author": "Jivan Goyal",
  "description": "Explore best technologies and techniques at one place",
  "license": "MIT",
  "packOptions": {
    "owners": [
      "Jivan Goyal"
    ],
    "summary": "Demo using AspNetCore,Mvc,Razor,Kestrel",
    "requireLicenseAcceptance": true,
    "licenseUrl": "https://github.com/jivangoyal/Demo-master/blob/master/LICENSE",
    "tags": [
      "AspNetCore",
      "Mvc",
      "Razor",
      "Kestrel",
      "Sass"
    ],
    "repository": {
      "url": "https://github.com/jivangoyal/Demo-master.git",
      "type": "git"
    }
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.0.0",
      "type": "platform"
    },
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.AspNetCore.Razor": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Node.js": "5.3.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-sass": "^2.2.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "debugType": "portable"
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "AspNetCoreApp": {
    "commandName": "Project",
    "launchBrowser": true,
    "launchUrl": "http://localhost:50000",
    "environmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    }
  },
  "publishOptions": {
    "include": [
      "docker-compose.yml",
      "docker-compose.debug.yml",
      "Dockerfile.debug",
      "Dockerfile",
      ".dockerignore"
    ]
  }
}